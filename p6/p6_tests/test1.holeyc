int bar(int one, int two, int three)
{
  one = 100;
  two = 200;
  three = 300;
  while(true)
  {
    if(one < two)
    {
      while(one < two)
      {
        one++;
      }
    }
    while(three > one)
    {
      one++;
    }
    while(one != 1)
    {
      one--;
    }
    if(one == 1)
    {
      return one;
    }
    else
    {
      return 0;
    }
  }
}