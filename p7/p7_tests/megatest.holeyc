int global_int;
char global_char;
bool global_bool;

bool try_to_init()
{
    global_int = 999;
    global_char = 'c;
    global_bool = true;
    return global_bool;
}

void RunTheProgram()
{
    bool init;
    init = try_to_init();
    if(init)
    {
        while(global_int > 10)
        {
            global_int--;
        }
        int triangle_area;
        triangle_area = 0;
        int side_one;
        int side_two;
        int side_three;
        side_one = global_int;
        side_two = global_int + 1;
        side_two = global_int - (global_int / 2);
        triangle_area = side_one * side_two * side_three;
        return;
    }
}

int main(){
  RunTheProgram();
}